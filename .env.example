# Friday AI Runtime — Environment Variables
#
# Copy this file to .env and fill in your API keys.
# Only ANTHROPIC_API_KEY is required.

# Required — Claude API key
ANTHROPIC_API_KEY=

# Optional — Multi-modal providers
OPENAI_API_KEY=
GOOGLE_API_KEY=
ELEVENLABS_API_KEY=

# Optional — Server port (default: 8787)
FRIDAY_PORT=8787
